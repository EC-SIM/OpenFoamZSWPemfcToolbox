FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      YO2air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "$FOAM_CASE/boundaryConditions/Air"

dimensions      [0 0 0 0 0 0 0];

internalField   uniform $O2InC;

boundaryField
{
    cat_in
    {
        type            fixedValue;
        value           $internalField;
    }
    
    cat_out
    {
        type            zeroGradient;
    }
    
    ".*symmetry.*"
    {
        type            symmetry;
    }
    
    ".*wall.*"
    {
        type            zeroGradient;
    }
    
    ".*_to_.*"
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}


FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fuel";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "$FOAM_CASE/boundaryConditions/Coolant"

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $TInCool; 

boundaryField
{
    coolant_in
    {
        type            fixedValue;
        value           $internalField;
    }
    
    coolant_out
    {
        type            zeroGradient;
    }
    
    ".*symmetry.*"
    {
        type            symmetry;
    }
    
    ".*_to_.*"
    {
       type            zeroGradient;
    }
    
    ".*periodic.*"
    {
        type            symmetry;
    }

}

